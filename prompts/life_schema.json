    "evolution": {
      "type": "object",
      "description": "HOW THE PERSON HAS CHANGED - Long-term trajectories across entire timeline (earliest period to most recent)",
      "properties": {
        "big5": {
          "type": "object",
          "description": "Big Five trait evolution from earliest to most recent periods",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "trajectory": {"type": "string", "enum": ["sustained_improvement", "sustained_decline", "stable", "U-shaped", "inverted-U"], "description": "Overall trajectory pattern across timeline"},
              "earliest_average": {"type": "number", "minimum": 0, "maximum": 1, "description": "Average score in earliest periods"},
              "recent_average": {"type": "number", "minimum": 0, "maximum": 1, "description": "Average score in most recent periods"},
              "change": {"type": "number", "description": "Change magnitude (recent_avg - early_avg)"},
              "change_type": {"type": "string", "description": "Significance of change (e.g., 'major_healing', 'minor_shift', 'stable')"},
              "turning_points": {
                "type": "array",
                "description": "Periods with significant shifts",
                "items": {
                  "type": "object",
                  "properties": {
                    "period": {"type": "string", "description": "When the shift occurred"},
                    "event": {"type": "string", "description": "What happened"},
                    "before": {"type": "number", "minimum": 0, "maximum": 1, "description": "Score before"},
                    "after": {"type": "number", "minimum": 0, "maximum": 1, "description": "Score after"}
                  },
                  "required": ["period", "event", "before", "after"]
                }
              },
              "narrative": {"type": "string", "description": "Story of growth/change for this trait"},
              "aspect_evolution": {
                "type": "object",
                "description": "How each aspect evolved separately (allows tracking split development - e.g., Intellect increased but Openness to Experience stayed stable)",
                "additionalProperties": {
                  "type": "object",
                  "properties": {
                    "trajectory": {"type": "string", "description": "Aspect trajectory (e.g., 'increased', 'decreased', 'stable')"},
                    "change": {"type": "string", "description": "Narrative of change (e.g., 'increased from 0.6 to 0.75')"}
                  },
                  "required": ["trajectory", "change"]
                }
              }
            },
            "required": ["trajectory", "earliest_average", "recent_average", "change", "change_type", "narrative", "aspect_evolution"]
          }
        },
        "attachment": {
          "type": "object",
          "description": "Long-term attachment evolution (movement toward/away from secure attachment)",
          "properties": {
            "trajectory": {"type": "string", "description": "Overall pattern (e.g., 'healing toward secure', 'increasing anxiety', 'stable anxious')"},
            "notes": {"type": "string", "description": "Anxiety/avoidance dimension changes, healing progress"}
          },
          "required": ["trajectory", "notes"],
          "additionalProperties": false
        },
        "schwartz_values": {
          "type": "object",
          "description": "Value system evolution over time",
          "properties": {
            "trajectory": {"type": "string", "description": "Overall pattern (which values strengthened/weakened, value system maturation)"},
            "notes": {"type": "string", "description": "Specific value shifts and value conflicts"}
          },
          "required": ["trajectory", "notes"],
          "additionalProperties": false
        },
        "timeline_phases": {
